#include <stdio.h>

int main() {
    int n, i, arr[100];
    printf("Enter the number of elements: ");
    scanf("%d", &n);

    printf("Enter the elements: \n");
    for (i = 0; i < n; i++)
        scanf("%d", &arr[i]);

    printf("Negative numbers: \n");
    for (i = 0; i < n; i++)
        if (arr[i] < 0)
            printf("%d\n", arr[i]);

    printf("Positive numbers: \n");
    for (i = 0; i < n; i++)
        if (arr[i] >= 0)
            printf("%d\n", arr[i]);

    return 0;
}
